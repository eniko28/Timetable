<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <title>Groups</title>
</head>

<body>

    <h1>Groups: </h1>
    <% groups.forEach(function(item) { %>
        <div>
            <h2>
                <%= item.id %>
            </h2>
            <p>Grade level:
                <%= item.gradeLevel %>
            </p>
            <div></div>
        </div>
        <% }); %>

</body>

</html>