<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <title>Subjects</title>
    <link rel="stylesheet" href="subjectsStyle.css">
</head>

<body>

    <h1>Registered users: </h1>
    <div class="container">
        <% users.sort((a, b)=> a.name.localeCompare(b.name)).forEach(function(item) { %>
            <div class="sub-cont">
                <h2>
                    <%= item.name %>
                </h2>
                <p>User Id:
                    <%= item.userId %>
                </p>
                <p>User type: <%= item.type %>
                </p>
                <div></div>
            </div>
            <% }); %>
    </div>


</body>

</html>